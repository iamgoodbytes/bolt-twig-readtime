{
    "name": "goodbytes/readtime",
    "description": "A simple twig filter that shows the average read time for a blog post",
    "type": "bolt-extension",
    "keywords": [ "twig", "readtime", "filter" ],
    "minimum-stability": "dev",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "GoodBytes",
            "email": "yeah@no.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\GoodBytes\\ReadTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\GoodBytes\\ReadTime\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\GoodBytes\\ReadTime\\ReadTimeExtension"
    }
}
